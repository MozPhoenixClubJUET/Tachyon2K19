  //  Program to print all the elements of a predefined vector in ascending  order separated by a blank space.
  #include<iostream>
   using namespace std;
   int main(void)
   {
    vector<int>v{1,4,3,2,5};
    sort(v.begin(),v.end());
    for(int i=v.begin();i<=v.end();i++)
    cout<<v[i]<<" ";
    return 0;
   }

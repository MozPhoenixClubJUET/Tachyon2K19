   //	Given an array with predefined integers,your task is to find the sum of all elements of array using STL
   #include <bits/stdc++.h>
   using namespace std;
   int main()
   {
	  int arr[] = { 1, 45, 54, 71, 76, 12 };
	  int n = sizeof(arr) / sizeof(arr[0]);
	  int sum=0;
	  cout << "Array: ";
	  for (int i = 0; i < n; i++)
	   {
		cout << arr[i] << " ";
		sum=sum+arr[i];
	   }
    cout << "\nSum = "<<sum ;
       return 0;
   }
